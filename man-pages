	* pkginst(1)
	* pkgup(1)
	* pkgremove(1)
	* liblist(1)
	* pkgcheck(1)
	* pkglist(1)
	* pkgfind(1)
	* libcheck(1)
	* libprecise(1)
	* restore-yalpack(5)
	* newfiles-yalpack(5)
